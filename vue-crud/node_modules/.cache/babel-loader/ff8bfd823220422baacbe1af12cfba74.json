\n\n    methods: {\n        async addCart() {\n            this.userID = supabase.auth.user().id\n            await supabase.from('cart').insert([{user_id: String(this.userID), item: `${this.title}`, price: this.price}])\n            const { data } = await supabase.from('cart')\n            console.log(data)\n        }\n    },\n\n\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":""}]}