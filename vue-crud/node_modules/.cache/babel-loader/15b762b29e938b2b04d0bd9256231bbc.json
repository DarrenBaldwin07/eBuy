nents: {\n    auth\n  },\n\n  data() {\n    return {\n      logo: logo,\n      currentImg: 0,\n      currentText: 0,\n      rightCounter: 1.5,\n      images: [\n        login3,\n        login2,\n        login1,\n      ],\n\n      text: [\n        'Find the best products at the greatest price.',\n        'Easy payment process.',\n        'Quick delivery.',\n      ]\n      \n    }\n  },\n\n  // called when view/page loads loads\n  mounted() {\n    this.animateImgs()\n  },\n\n  methods: {\n    animateImgs() {\n      setInterval(() => {\n\n        this.currentImg++\n        this.currentText++\n\n        if (this.currentImg === 3) {\n          this.currentImg = 0\n          this.currentText = 0\n        }\n      }, 2000)\n    },\n\n    \n  },\n}\n</script>\n\n<style>\n\n</style>"],"sourceRoot":""}]}